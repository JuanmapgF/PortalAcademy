package Modelo;

import java.util.List;

public class Administrador{
	public static void main(String[] args) {
		BD bd = new BD();
		List<Object[]> aux = bd.Select("SELECT * FROM Usuario");
		Object[] user1 = aux.get(0);
		Object[] user2 = aux.get(1);
		for (Object object : user1) {
			System.out.println("Nick: "+object.toString());
		}
		bd.finalize();
	}
}
